<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>EA_Questionnaire_de_satisfaction</fullName>
        <description>EA Questionnaire de satisfaction</description>
        <protected>false</protected>
        <recipients>
            <field>Tech_Email__c</field>
            <type>email</type>
        </recipients>
        <senderType>DefaultWorkflowUser</senderType>
        <template>Mails_Pan_listes_PRM/Questionnaire_de_satisfaction</template>
    </alerts>
    <alerts>
        <fullName>EA_Questionnaire_post_chute_Mediamat</fullName>
        <description>EA Questionnaire post-chute Mediamat</description>
        <protected>false</protected>
        <recipients>
            <field>ContactId</field>
            <type>contactLookup</type>
        </recipients>
        <senderAddress>panelmediamat@mediametrie.fr</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
        <template>Mails_Pan_listes_PRM/Questionnaire_post_chute_Mediamat</template>
    </alerts>
    <alerts>
        <fullName>EA_Questionnaire_post_chute_PaME</fullName>
        <description>EA Questionnaire post-chute PaME</description>
        <protected>false</protected>
        <recipients>
            <field>ContactId</field>
            <type>contactLookup</type>
        </recipients>
        <senderAddress>sclaret@mediametrie.fr</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
        <template>Mails_Pan_listes_PRM/Questionnaire_post_chute_PaME</template>
    </alerts>
    <fieldUpdates>
        <fullName>FU_Priorite_intervention_basse</fullName>
        <field>Priority</field>
        <literalValue>Bas</literalValue>
        <name>FU_Priorité_intervention_basse</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>FU_Priorite_intervention_haute</fullName>
        <field>Priority</field>
        <literalValue>Elevé</literalValue>
        <name>FU_Priorité_intervention_haute</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <rules>
        <fullName>WF01_WOR_Priorité_haute</fullName>
        <actions>
            <name>FU_Priorite_intervention_haute</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( 				$User.Bypass_workflows__c =false, 				OR ( 								TEXT(Sous_type_d_intervention__c) = $Label.WOR_Sous_type_d_intervention_Demenagement, 								TEXT(Sous_type_d_intervention__c) = $Label.WOR_Sous_type_d_intervention_Controle_de_connexion,         TEXT(Sous_type_d_intervention__c) = $Label.WOR_Sous_type_d_intervention_Reinstallation, 								TEXT(Sous_type_d_intervention__c) = $Label.WOR_Sous_type_d_intervention_Controle_d_audience 				) )</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>WF02_WOR_Priorité_basse</fullName>
        <actions>
            <name>FU_Priorite_intervention_basse</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( 				$User.Bypass_workflows__c =false, 				OR( 								TEXT(Sous_type_d_intervention__c) = $Label.WOR_Sous_type_d_intervention_Visite_de_courtoisie, 				    TEXT(Sous_type_d_intervention__c) = $Label.WOR_Sous_type_d_intervention_Suppression_d_un_poste 				) )</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>WF03_WOR_Envoie_questionnaire</fullName>
        <actions>
            <name>EA_Questionnaire_de_satisfaction</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>User.Bypass_workflows__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Status</field>
            <operation>equals</operation>
            <value>Terminée</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Sous_type_d_intervention__c</field>
            <operation>notEqual</operation>
            <value>8</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>WF05_WOR_Envoi_questionnaire_post_chute_Mediamat</fullName>
        <actions>
            <name>EA_Questionnaire_post_chute_Mediamat</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>User.Bypass_workflows__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Status</field>
            <operation>equals</operation>
            <value>Terminée</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Sous_type_d_intervention__c</field>
            <operation>equals</operation>
            <value>8</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Type_de_panel__c</field>
            <operation>equals</operation>
            <value>MEDIAMAT (MMAT)</value>
        </criteriaItems>
        <description>Envoi d&apos;un questionnaire de satisfaction après une intervention de désinstallation définitive</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>WF06_WOR_Envoi_questionnaire_post_chute_PaME</fullName>
        <actions>
            <name>EA_Questionnaire_post_chute_PaME</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>User.Bypass_workflows__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Status</field>
            <operation>equals</operation>
            <value>Terminée</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Sous_type_d_intervention__c</field>
            <operation>equals</operation>
            <value>8</value>
        </criteriaItems>
        <criteriaItems>
            <field>WorkOrder.Type_de_panel__c</field>
            <operation>equals</operation>
            <value>PAME</value>
        </criteriaItems>
        <description>Envoi d&apos;un questionnaire de satisfaction après une intervention de désinstallation définitive</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
</Workflow>
